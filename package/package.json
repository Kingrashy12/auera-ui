{
  "name": "auera-ui",
  "version": "0.1.24",
  "main": "dist/auera-ui.cjs",
  "module": "dist/auera-ui.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/auera-ui.es.js",
      "require": "./dist/auera-ui.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/auera.css": "./dist/auera.css"
  },
  "scripts": {
    "build": "rm -rf .rollup.cache && rm -rf tsconfig.tsbuildinfo && rollup -c",
    "insert": "node scripts/insert-client.cjs",
    "postbuild": "npm run insert",
    "eject": "node scripts/eject.js"
  },
  "bin": {
    "auera-ui": "./scripts/cli.js"
  },
  "files": [
    "dist",
    "dist/types",
    "dist/auera.css",
    "scripts"
  ],
  "keywords": [
    "react",
    "ui-kit",
    "design-system",
    "react-component",
    "tailwindcss"
  ],
  "author": "Rapheal Chizitere",
  "license": "Apache-2.0",
  "description": "A robust UI library providing flexible, high-quality components, built with Tailwind CSS",
  "homepage": "https://aueraui.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kingrashy12/auera-ui"
  },
  "bugs": {
    "url": "https://github.com/Kingrashy12/auera-ui/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0 || ^19.0.0-rc",
    "react-dom": ">=18.0.0 || ^19.0.0-rc",
    "react-icons": "^5.3.0",
    "tailwindcss": ">=3.4.15"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.25.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/react": "^18.3.12",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.49",
    "rollup": "^4.27.4",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "arg": "^5.0.2",
    "motion": "^11.15.0",
    "stywind": "^0.1.9"
  }
}
